{
  "env": {
    "development": {
      "presets": [
        "@babel/env",
        "@babel/typescript",
        ["@babel/preset-react", { "development": true }]
      ],
      "plugins": [
        "react-hot-loader/babel",
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-proposal-object-rest-spread",
        "@babel/plugin-proposal-optional-chaining"
      ]
    },
    "production": {
  	  "presets": [
        "@babel/env",
  	    "@babel/preset-react"
      ],
      "plugins": [
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-proposal-object-rest-spread",
        "@babel/plugin-proposal-optional-chaining"
      ]
    }
  }
}
